{
  "manifest_version": 3,
  "name": "Burnout Tracker",
  "version": "1.3",
  "description": "Tracks your browsing and calendar data to help detect burnout. Data is synced immediately to Google Sheets.",
  "permissions": [
    "storage",
    "tabs",
    "identity",
    "idle",
    "alarms",
    "notifications",
    "webRequest"
  ],
  "host_permissions": [
    "<all_urls>",
    "https://script.google.com/*",
    "https://www.googleapis.com/*"
  ],
  "oauth2": {
    "client_id": "771547579484-ora5jhdsfk1gu0cu6jae91hoihjkedis.apps.googleusercontent.com",
    "scopes": [
      "https://www.googleapis.com/auth/calendar.readonly"
    ]
  },
  "chrome_url_overrides": {
    "newtab": "questions.html"
  },
  "background": {
    "service_worker": "background.js"
  }
  
  
}
